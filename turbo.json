{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*"]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**/*"]
    },
    "types:check": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["^deploy"],
      "outputs": []
    },
    "//#lint": {
      "outputs": []
    },
    "//#audit-packages": {
      "dependsOn": [],
      "inputs": ["yarn.lock"],
      "outputs": []
    }
  }
}
